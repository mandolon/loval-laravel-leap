# Storage mode (switch to false when ready for backend)
VITE_USE_LOCAL_STORAGE=true

# API configuration (for when you switch to Laravel/Supabase)
VITE_API_BASE_URL=http://127.0.0.1:8000/api
VITE_API_VERSION=v1

# Debug mode (logs API operations to console)
VITE_DEBUG_API=false

# Authentication mode
VITE_AUTH_MODE=supabase
